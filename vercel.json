{
  "version": 2,
  "builds": [
    {
      "src": "ICP_Prompt_Tester/api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "ICP_Prompt_Tester/api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "ICP_Prompt_Tester/api/questions.js",
      "use": "@vercel/node"
    },
    {
      "src": "ICP_Prompt_Tester/api/prompts.js",
      "use": "@vercel/node"
    },
    {
      "src": "ICP_Prompt_Tester/api/prompts/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "ICP_Prompt_Tester/api/results.js",
      "use": "@vercel/node"
    },
    {
      "src": "ICP_Prompt_Tester/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "ICP_Prompt_Tester/api/test.js"
    },
    {
      "src": "/api/health",
      "dest": "ICP_Prompt_Tester/api/health.js"
    },
    {
      "src": "/api/questions",
      "dest": "ICP_Prompt_Tester/api/questions.js"
    },
    {
      "src": "/api/prompts",
      "dest": "ICP_Prompt_Tester/api/prompts.js"
    },
    {
      "src": "/api/prompts/test",
      "dest": "ICP_Prompt_Tester/api/prompts/test.js"
    },
    {
      "src": "/api/results",
      "dest": "ICP_Prompt_Tester/api/results.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg))",
      "dest": "$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "installCommand": "npm install && cd ICP_Prompt_Tester && npm install && cd ICP_Prompt_Tester/api && npm install",
  "buildCommand": "cd ICP_Prompt_Tester && npm run build",
  "outputDirectory": "ICP_Prompt_Tester/build"
}