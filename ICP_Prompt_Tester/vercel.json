{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "installCommand": "npm install && cd ICP_Prompt_Tester && npm install",
  "buildCommand": "cd ICP_Prompt_Tester && npm run build",
  "outputDirectory": "ICP_Prompt_Tester/build",
  "env": {
    "REACT_APP_API_URL": "@api_url"
  }
}
